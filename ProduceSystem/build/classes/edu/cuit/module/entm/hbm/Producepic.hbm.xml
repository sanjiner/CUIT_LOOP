<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.cuit.module.entm.entity.Producepic" table="producepic" catalog="ProduceSystem" optimistic-lock="version">
        <id name="id" type="string">
            <column name="id" />
            <generator class="assigned"/>
            </id>
        <property name="applyBh" type="string">
            <column name="applybh"  length="50"/>
        </property>
        <property name="picName" type="string">
            <column name="picname"  length="50"/>
        </property>
        <property name="picsrc" type="string">
            <column name="picsrc"  length="100"/>
        </property>
        <property name="picContent" type="string">
            <column name="piccontent"  length="65535"/>
        </property>
        <property name="time" type="string">
            <column name="time"  length="50"/>
        </property>
        </class>
    </hibernate-mapping>